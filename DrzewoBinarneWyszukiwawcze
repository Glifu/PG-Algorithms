void wybor(char wejscie) {
	switch (wejscie)
	{
	case '+':
		char data;
		cin >> data;
		dodaj(data);
		break;

	case '<':
		minimum();
		break;

	case '>':
		maximum();
		break;

	case 'h':
		
		break;

	case '1':

		break;

	case '2':

		break;

	default:
		break;
	}
}

